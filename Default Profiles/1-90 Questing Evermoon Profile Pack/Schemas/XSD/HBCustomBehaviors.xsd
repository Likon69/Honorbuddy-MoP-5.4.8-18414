  <xs:complexType name="QuestBehaviorBase">
    <xs:sequence>
      <xs:element minOccurs="0" ref="AvoidMobs"/>
      <xs:group minOccurs="0" ref="QBCoreSubElement-Blackspots"/>
    </xs:sequence>
    <xs:attributeGroup ref="Attributes-QuestSpecifier"/>
    <xs:attribute name="IgnoreMobsInBlackspots" type="HBType-Boolean"/>
    <xs:attribute name="MovementBy" type="QBCoreType-MovementBy"/>
